<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gsgg.gsggbe.mapper.logIn.JoinMapper">
    <select id="isExist" parameterType="String" resultType="int">
        select count(a.id) from user a where a.id = #{userId}
    </select>
    <insert id="saveUser" parameterType="com.gsgg.gsggbe.login.dto.JoinDTO">
        insert into user (id,password,username,role) values (#{userId},#{password},#{username},#{role})
    </insert>
</mapper>